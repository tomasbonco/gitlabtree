{
  "name": "GitLab - Tree view",
  "homepage_url": "https://github.com/tomasbonco/gitlabtree",
  "author": "Tomáš Bončo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Provides folder structure view for code review.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ]
}